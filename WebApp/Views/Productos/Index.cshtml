@model IEnumerable<WebApp.Models.Producto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicioVigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinVigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Beneficios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Requisitos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Terminos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicioVigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinVigencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Beneficios.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Requisitos.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Terminos.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

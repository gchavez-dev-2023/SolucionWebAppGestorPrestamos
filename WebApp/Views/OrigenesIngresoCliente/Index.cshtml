@model IEnumerable<WebApp.Models.OrigenIngresoCliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicioActividad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFinActividad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoLiquidoPercibido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrlDocumento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoActividad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicioActividad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinActividad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoLiquidoPercibido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrlDocumento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoActividad.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
